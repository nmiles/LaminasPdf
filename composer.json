{
  "name": "nmiles/laminas-pdf",
  "description": "Laminas Pdf Component",
  "type": "library",
  "license": "BSD-3-Clause",
  "keywords": [
    "laminas",
    "pdf"
  ],
  "require": {
    "php": ">=7.4",
    "laminas/laminas-memory": ">=2.0.0",
    "laminas/laminas-stdlib": ">=2.0.0"
  },
  "require-dev": {
    "laminas/laminas-coding-standard": "~1.0.0",
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "LaminasPdf\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LaminasPdfTest\\": "test/"
    }
  },
  "replace": {
    "zendframework/zendpdf": "^2.0.0"
  }
}
