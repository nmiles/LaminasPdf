{
  "name": "nmiles/laminas-pdf",
  "description": "Laminas Pdf Component",
  "type": "library",
  "license": "BSD-3-Clause",
  "keywords": [
    "laminas",
    "pdf"
  ],
  "require": {
    "php": ">=8.0",
    "laminas/laminas-memory": "^2.9",
    "laminas/laminas-stdlib": ">=2.0.0"
  },
  "require-dev": {
    "laminas/laminas-coding-standard": "^2.1.4",
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^0.12.81"
  },
  "autoload": {
    "psr-4": {
      "LaminasPdf\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LaminasPdfTest\\": "test/"
    }
  },
  "replace": {
    "zendframework/zendpdf": "^2.0.0"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
